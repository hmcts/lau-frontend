{# Asynchronously load the CSS #}

<link id="main-css" rel="preload" href="/<%= htmlWebpackPlugin.files.css[0].substr(htmlWebpackPlugin.files.publicPath.length) %>" as="style">
<noscript><link rel="stylesheet" href="/<%= htmlWebpackPlugin.files.css[0].substr(htmlWebpackPlugin.files.publicPath.length) %>"></noscript>

<script nonce="{{nonce}}">
  document.getElementById("main-css").addEventListener("load", function () {
    this.onload=null; this.rel='stylesheet'
  });
</script>

<link id="flatpickr-css" rel="preload" href="assets/css/flatpickr.min.css" as="style">
<noscript><link rel="stylesheet" href="assets/css/flatpickr.min.css"></noscript>

<script nonce="{{nonce}}">
  document.getElementById("flatpickr-css").addEventListener("load", function () {
    this.onload=null; this.rel='stylesheet'
  });
</script>
