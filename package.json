    "@types/lodash": "^4.14.165",
    "@types/node": "^20.8.2",
    "@types/node-fetch": "^2.6.6",
    "@types/nunjucks": "^3.1.3",
    "@types/require-directory": "^2.1.1",
    "@types/serve-favicon": "^2.5.1",
    "@types/totp-generator": "^0.0.6",
    "applicationinsights": "^2.7.0",
    "autobind-decorator": "^2.4.0",
    "compression": "^1.7.4",
