    "html_codesniffer": "^2.5.1",
    "jest": "^29.3.1",
    "mini-css-extract-plugin": "^2.2.0",
    "mocha": "^11.0.0",
    "mocha-multi": "^1.1.6",
    "mochawesome": "^7.1.2",
    "nock": "^14.0.0",
    "nodemon": "^3.0.0",
    "pa11y": "^8.0.0",
    "puppeteer": "^24.0.0",
    "sass": "^1.63.4",
    "sass-loader": "^14.0.0",
    "sinon": "^19.0.0",
